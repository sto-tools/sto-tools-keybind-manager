{
  "name": "sto-keybind-manager-tests",
  "version": "1.0.0",
  "description": "Test suite for STO Tools Keybind Manager",
  "main": "cli-runner.js",
  "scripts": {
    "test": "node cli-runner.js",
    "test:verbose": "node cli-runner.js --verbose",
    "test:unit": "node cli-runner.js --filter \"Module\"",
    "test:integration": "node cli-runner.js --filter \"Integration\"",
    "test:e2e": "node run-e2e.js",
    "test:all": "npm test && npm run test:e2e",
    "test:json": "node cli-runner.js --reporter json --output results.json",
    "test:junit": "node cli-runner.js --reporter junit --output results.xml",
    "test:tap": "node cli-runner.js --reporter tap --output results.tap",
    "test:ci": "node cli-runner.js --reporter junit --output test-results.xml --stop-on-failure",
    "test:watch": "nodemon --watch ../js --watch suites --exec \"npm test\"",
    "install-deps": "npm install",
    "help": "node cli-runner.js --help"
  },
  "keywords": [
    "testing",
    "sto",
    "keybind",
    "star-trek-online",
    "automation"
  ],
  "author": "STO Tools Keybind Manager",
  "license": "MIT",
  "dependencies": {
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "bin": {
    "sto-test": "./cli-runner.js"
  }
} 